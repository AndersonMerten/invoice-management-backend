{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/vercel.ts"
    }
  ],
    "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@2.0.0"
    }
  }
}