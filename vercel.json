{
  "version": 2,
  "buildCommand": "prisma generate && nest build --webpack",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "api/vercel.ts",
      "use": "@vercel/node@2.0.0",
      "config": {
        "includeFiles": ["dist/**"],
        "tsconfig": "tsconfig.json"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}