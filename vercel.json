{
  "version": 2,
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm prisma generate && pnpm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "api/vercel.ts",
      "use": "@vercel/node@2.0.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}